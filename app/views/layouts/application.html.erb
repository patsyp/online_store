<!DOCTYPE html>
<html>
<head>
  <title>Exam</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if session[:user_id] %>
	<a href="/sessions/<%=session[:user_id]%>" data-method='delete'>Logout</a>
<%end%>
<%= yield %>

</body>
</html>
